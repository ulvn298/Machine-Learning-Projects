        "from sklearn.ensemble import RandomForestClassifier\n",
        "\n",
        "X = [[25, 20000], [45, 80000], [20, 15000], [50, 100000]]\n",
        "\n",
        "y = [0, 1, 0, 1]\n",
        "\n",
        "clf = RandomForestClassifier()\n",
        "clf.fit(X, y)\n",
        "\n",
        "print(f\"نتيجة القرض: {clf.predict([[30, 50000]])}\")"
      ]
    }
  ]
